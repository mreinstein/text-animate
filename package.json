{
  "name": "text-animate",
  "version": "0.0.2",
  "description": "beautiful, animated, HTML typographic UI effects",
  "main": "index.js",
  "scripts": {
    "build": "browserify example/index.js -t [ babelify --presets [ es2015 ] ] | uglifyjs --screw-ie8 --compress > example/bundle.js",
    "watch": "watchify example/index.js -v -o example/bundle.js --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mreinstein/text-animate.git"
  },
  "keywords": [
    "text",
    "effects",
    "animation",
    "typography",
    "typographic",
    "ui"
  ],
  "author": "Mike Reinstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mreinstein/text-animate/issues"
  },
  "homepage": "https://github.com/mreinstein/text-animate#readme",
  "dependencies": {
    "clamp": "^1.0.1",
    "eases": "^1.0.8",
    "raf": "^3.3.0",
    "seedrandom": "^2.4.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.22.0",
    "babelify": "^7.3.0",
    "uglify-js": "^2.7.5",
    "watchify": "^3.9.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
